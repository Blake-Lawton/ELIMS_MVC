}

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="box-title">
            <b>Users</b>
        </h3>
    </div>
     /.box-header 
    <div class="panel-body">
        <table class="table table-hover table-bordered table-condensed" id="UserList">
